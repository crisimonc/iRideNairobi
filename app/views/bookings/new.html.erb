<div class="container padded-top">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= simple_form_for [@motorcycle, @booking] do |f| %>
        <%= f.input :address %>
        <%= f.submit "request booking", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>


<div
  id="map"
  style="width: 100%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>"
>
</div>
